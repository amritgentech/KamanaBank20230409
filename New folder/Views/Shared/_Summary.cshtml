                        <div class="col-xs-5">
                            <div class="icon-big icon-danger text-center">
                                <i><img class="imageSize" src="../../Content/images/invalid.png" /></i>
                            </div>
                        </div>
                        <div class="col-xs-7">
                            <div class="numbers">
                                <p>@Model.Invalid</p>
                                <p>
                                    <b>Npn Invalid</b>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </a>
    </div>
